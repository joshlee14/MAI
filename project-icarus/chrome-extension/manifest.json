{
  "manifest_version": 3,
  "name": "Project Icarus",
  "description": "Medicare Advantage plan comparison and AI closing assistant",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html",
    "default_title": "Medicare Plan Finder"
  },
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}